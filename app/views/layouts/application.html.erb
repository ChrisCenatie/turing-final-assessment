<!DOCTYPE html>
<html>
<head>
  <title>FinalAssessment</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => false %>
  <%= csrf_meta_tags %>
</head>
  <body>
    <div class="container">
      <% if current_user %>
        <div class="btn btn-default btn-sm active" role="button">
          <%= link_to "Sign Out", logout_path, method: :delete, style: "color:white;" %>
        </div>
      <% end %>
      <%= yield %>
    </div>
  </body>
</html>
